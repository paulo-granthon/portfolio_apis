{
  "name": "portfolio_apis",
  "version": "1.0.0",
  "scripts": {
    "dev": "concurrently \"yarn api-dev\" \"yarn web-dev\"",
    "api-dev": "make bd",
    "web-dev": "make fd",
    "prod": "concurrently \"yarn api-prod\" \"yarn web-prod\"",
    "api-prod": "make bp",
    "web-prod": "make fp",
