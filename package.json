    "db-up": "docker-compose up -d",
    "db-down": "docker-compose down"
  },
  "repository": "https://github.com/paulo-granthon/portfolio_apis.git",
  "author": "Paulo Granthon <paulo.granthon@fatec.sp.gov.br>",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^8.2.2"
  }
}
